{
    "Main": {
        "OutFile": "test_addmodels_v2.json",
        "Repeat": 1,
        "UseModularStructure": true,
        "ProcessMLPipeline": true,
        "FitModels": true,
        "PostFitAnalysis": true
    },
    "Independent": {
        "Source": "generator",
        "Generator": {
            "Type": "equations",
            "DataPoints": 20,
            "Variables": 2,
            "Equations": "0.001|(X - 1) * 0.0001"
        }
    },
